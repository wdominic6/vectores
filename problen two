#include <iostream>
#include <vector>
using namespace std;
int main(){
    int n,s;
    cin>>n;
    vector<int> vec;
    for(int i=1; i<=n; i++){
        vec.push_back(i);
    }
    cout<<"primeros "<<n<<" numeros naturales: "<<endl;
    for(int i=0;i<vec.size();i++){
        s=s+vec[i];
    }
    cout<<s<<endl;
    return 0;
}
